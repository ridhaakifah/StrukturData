#include <iostream>
#include <string>
using namespace std;

struct Mahasiswa {
    string nama;
    string NIM;
};

int main () {
    Mahasiswa mhs;
    // mhs.nama = "Idaa";
    // mhs.NIM = "103112400132";

    cout << "Nama : ";
    getline(cin, mhs.nama);
    cout << "NIM : ";
    cin >> mhs.NIM;

    cout << "Nama : " << mhs.nama << endl
         << "NIM : " << mhs.NIM;
}
